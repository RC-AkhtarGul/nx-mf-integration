{
  "name": "login",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "login/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rspack:rspack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "login/dist",
        "index": "login/src/index.html",
        "baseHref": "/",
        "main": "login/src/main.ts",
        "tsConfig": "login/tsconfig.app.json",
        "assets": [
          "login/src/favicon.ico",
           "login/src/assets",
           {
            "input": "login/public",
            "glob": "**/*",
            "output": "."
          }
          ],
        "rspackConfig": "login/rspack.config.ts",
        "styles": ["login/src/styles.css"],
        "scripts": [],
        "configurations": {
          "development": {
            "mode": "development"
          },
          "production": {
            "mode": "production",
            "optimization": true,
            "sourceMap": false,
            "outputHashing": "all",
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false
          }
        },
        "target": "web"
      },
      "configurations": {
        "production": {
          "rspackConfig": "login/rspack.config.prod.ts"
        }
      },
      "dependsOn": ["^build", "typecheck"]
    },
    "serve": {
      "executor": "@nx/rspack:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "login:build",
        "hmr": true,
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "login:build:development"
        },
        "production": {
          "buildTarget": "login:build:production",
          "hmr": false
        }
      },
      "dependsOn": ["typecheck"]
    },
    "serve-static": {
      "executor": "@nx/rspack:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "login:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "login:serve:development"
        },
        "production": {
          "serveTarget": "login:serve:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{projectRoot}/test-output/jest/coverage"],
      "options": {
        "jestConfig": "login/jest.config.ts"
      }
    }
  }
}
